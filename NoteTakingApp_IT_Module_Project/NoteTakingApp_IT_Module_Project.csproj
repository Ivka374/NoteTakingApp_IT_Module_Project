<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Font\Poppins-Bold.ttf" />
    <None Remove="Font\Poppins-BoldItalic.ttf" />
    <None Remove="Font\Poppins-Italic.ttf" />
    <None Remove="Font\Poppins-Regular.ttf" />
    <None Remove="Images\bold.png" />
    <None Remove="Images\close.png" />
    <None Remove="Images\colourPalette.png" />
    <None Remove="Images\delete.png" />
    <None Remove="Images\expand.png" />
